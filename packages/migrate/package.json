{
  "name": "tsdown-migrate",
  "version": "0.17.0-beta.2",
  "description": "A CLI tool to help migrate your project to tsdown.",
  "type": "module",
  "license": "MIT",
  "homepage": "http://tsdown.dev/",
  "bugs": {
    "url": "https://github.com/rolldown/tsdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolldown/tsdown.git",
    "directory": "packages/migrate"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./run": "./dist/run.mjs",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "bin": {
    "tsdown-migrate": "./dist/run.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "node ./src/run.ts"
  },
  "dependencies": {
    "@antfu/ni": "catalog:migrate",
    "ansis": "catalog:prod",
    "cac": "catalog:prod",
    "consola": "catalog:migrate",
    "diff": "catalog:migrate"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
